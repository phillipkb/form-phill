{
    "name": "form-phill",
    "manifest_version": 3,
    "description": "This extension allows you to auto-fill forms.",
    "author": "Phillip Kekoa bannister",
    "version": "1.0",
    "permissions": [
        "activeTab"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content_script.js"]
        }
    ]
}